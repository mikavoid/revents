[ignore]
.*/node_modules/.*
.*/semantic/.*
.*/src/serviceWorker\.js
.*/src/index\.js
.*/src/app/.*
.*\.test\.js

[include]

[libs]
declare module 'semantic-ui-react' {
   declare var exports: 'semantic-ui-react';
}

[lints]

[options]
all=true
module.system.node.resolve_dirname=src
module.name_mapper='^/\(.*\)$' -> '<PROJECT_ROOT>/src\1'

[strict]
